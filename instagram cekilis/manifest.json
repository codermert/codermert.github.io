{

    "name": "Instagram Çekiliş Yazılımı",
    "description": "Saniyeler İçerisinde Profesyonel, Güvenilir, Hızlı Instagram Çekilişi Yapın",
    "version": "1.0.8",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Instagram Çekiliş Yazılımı"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "cookies",
        "https://www.instagram.com/"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}